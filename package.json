  "bugs": {
    "url": "https://github.com/uzair-ashraf/raise-a-kitty/issues"
  },
  "homepage": "https://github.com/uzair-ashraf/raise-a-kitty#readme",
  "devDependencies": {
    "pg-bump": "^3.0.0"
  }
}
